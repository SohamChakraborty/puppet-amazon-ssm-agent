{
  "name": "shinesolutions-amazon_ssm_agent",
  "version": "0.9.3",
  "author": "Shine Solutions (opensource@shinesolutions.com)",
  "summary": "Puppet module for managing Amazon System Manager Agent(ssm agent)",
  "license": "Apache-2.0",
  "source": "https://github.com/shinesolutions/puppet-amazon-ssm-agent.git",
  "project_page": "https://github.com/shinesolutions/puppet-amazon-ssm-agent",
  "issues_url": "https://github.com/shinesolutions/puppet-amazon-ssm-agent/issues",
  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": "1.3.x"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": "2.2.0"
    }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
